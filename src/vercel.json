{
    "builds":[
        {
            "src":"/src/app.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/api/(.*)",
            "dest":"/src/app.js"
        },
        {
            "src":"/api/(.*)",
            "dest":"/src/app.js"
        },
        {
            "src":"/(.*)",
            "dest":"/src/app.js"
        }
    ]
}